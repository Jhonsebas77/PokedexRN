{
  "name": "PokedexRN",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "./scripts/start.sh",
    "test": "jest",
    "tsc": "tsc",
    "build": "npm run clean && gulp resource-copy && npm run tsc --",
    "rnstart": "react-native start --reset-cache",
    "watch": "npm run build -- -w",
    "emulator": "emulator @Nexus_6_API_28 -dns-server 8.8.8.8,8.8.4.4",
    "clean": "rimraf lib",
    "cleancache": "sudo npm cache clean -f",
    "cleanall": "yarn cache clean --force && rm -rf yarn.lock && npm cache clean --force && rm -rf node_modules/ && rm -rf package-lock.json && rm -rf ios/build && rm -rf android/app/build && git clean -xdf",
    "clock:android": "adb shell \"date `date +%m%d%H%M%Y.%S`\"",
    "pod": "cd ./ios && rm -rf Podfile.lock && rm -rf Pods && pod install",
    "ios": "npx react-native run-ios",
    "ios:pod": "cd ./ios && rm -rf Podfile.lock && rm -rf Pods && pod install",
    "iosX": "npx react-native run-ios --simulator \"iPhone ${1:-X}\"",
    "iosXr": "npx react-native run-ios --simulator \"iPhone ${1:-XR}\"",
    "android": "npx react-native run-android",
    "android:release": "npm run build && npx react-native run-android --variant=release",
    "android:shake": "adb shell input keyevent 82"
  },
  "dependencies": {
    "@babel/runtime": "7.6.2",
    "gulp": "3.9.1",
    "gulp-iconfont": "10.0.*",
    "gulp-typescript": "5.0.*",
    "lottie-react-native": "^3.3.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-router-flux": "^4.2.0",
    "react-native-screens": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/plugin-proposal-decorators": "^7.4.*",
    "babel-plugin-module-resolver": "^3.2.*",
    "babel-plugin-root-import": "^6.1.*",
    "babel-plugin-transform-remove-console": "^6.9.*",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.33",
    "@types/react-native": "^0.62.2",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^24.9.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-react-native": "^4.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "haul": "^1.0.0-rc.15",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-native-bundle-visualizer": "^2.1.1",
    "react-native-reanimated": "^1.8.0",
    "react-navigation-tabs": "^2.8.11",
    "react-test-renderer": "16.11.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "typings": "^2.1.1"
  },
  "jest": {
    "preset": "react-native"
  }
